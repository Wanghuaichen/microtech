{
  "name": "micro-technologies",
  "version": "0.1.0",
  "private": true,
  "main": "server/server.js",
  "proxy": "http://localhost:4200",
  "build-css": "node-sass-chokidar src/ -o src/",
  "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js &&webpack-dev-server --inline --content-base . --history-api-fallback",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "massive": "^4.5.0",
    "material-ui": "^0.20.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "nuka-carousel": "^3.0.0",
    "prerender-node": "^2.7.4",
    "react": "^16.2.0",
    "react-animated-number": "^0.4.4",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-meta-tags": "^0.3.0",
    "react-modal-video": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-sitemap": "^1.1.3",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0",
    "video-react": "^0.9.3"
  }
}
